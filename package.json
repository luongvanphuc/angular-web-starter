{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "build:stats": "npm run build:prod -- --stats-json",
    "lint": "nodemon --exec \"npm run lint:all || exit 1\" --ext ts,html,scss",
    "lint-scripts": "nodemon --exec \"npm run lint:scripts || exit 1\" --ext ts",
    "lint-styles": "nodemon --exec \"npm run lint:all || exit 1\" --ext html,scss",
    "lint:all": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "bundle-report": "webpack-bundle-analyzer dist/web/stats.json",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~7.2.8",
    "@angular/compiler": "~7.2.8",
    "@angular/core": "~7.2.8",
    "@angular/forms": "~7.2.8",
    "@angular/platform-browser": "~7.2.8",
    "@angular/platform-browser-dynamic": "~7.2.8",
    "@angular/router": "~7.2.8",
    "@ng-bootstrap/ng-bootstrap": "^4.1.2",
    "core-js": "^2.5.4",
    "ngx-webstorage": "^3.0.2",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "~7.2.8",
    "@angular/language-service": "~7.2.8",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.11",
    "protractor": "~5.4.0",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-bundle-analyzer": "^3.3.2"
  }
